<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field">

    <class name="tourism.bean.TravelTeamInfo" table="TravelTeamInfo" schema="tourism">
        <id name="id" access="property">
            <column name="id" not-null="true">
                <comment>主键</comment>
            </column>
        </id>
        <property name="createTime" type="timestamp" update="false">
            <column name="create_time" sql-type="date" not-null="true" default="systimestamp">
                <comment>创建时间</comment>
            </column>
        </property>
        <property name="travelName" not-null="true">
            <column name="travelName">
                <comment>旅行团名称</comment>
            </column>
        </property>
        <property name="phone" not-null="true">
            <column name="phone">
                <comment>旅行团联系电话</comment>
            </column>
        </property>
        <property name="travelNo" not-null="true">
            <column name="travelNo">
                <comment>旅行团编号</comment>
            </column>
        </property>
        <property name="travelHead">
            <column name="travelHead">
                <comment>旅行团头像图片</comment>
            </column>
        </property>
        <property name="able">
            <column name="able">
                <comment>旅行团是否可用</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>